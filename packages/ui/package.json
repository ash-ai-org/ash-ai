{
  "name": "@ash-ai/ui",
  "version": "0.0.4",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css",
    "./styles-full.css": "./dist/styles-full.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ash-ai-org/ash-ai.git",
    "directory": "packages/ui"
  },
  "license": "MIT",
  "description": "React components for building Ash agent playground UIs",
  "keywords": [
    "ash",
    "ai",
    "agent",
    "playground",
    "react",
    "ui"
  ],
  "scripts": {
    "build": "tsup && pnpm build:css",
    "build:css": "postcss src/styles/index.css -o dist/styles-full.css && postcss src/styles/index-no-preflight.css -o dist/styles.css",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "@ash-ai/sdk": "workspace:*",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@ash-ai/sdk": "workspace:*",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  }
}
